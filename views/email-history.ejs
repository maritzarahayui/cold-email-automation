                        <div class="tab-pane fade show active" id="pills-home" role="tabpanel" aria-labelledby="pills-home-tab">
                            <div class="email-list dz-scroll" id="emails">                             
                            <!-- Dynamic emails from the handler function -->
                            <% if (emails && emails.length > 0) { %>
                                <% emails.forEach(email => { %>
                                        <div class="message">
                                            <a href="/detail/<%= email.id %>" class="col-mail col-mail-2">
                                                <div class="hader" style="color: #FF9F00"><%= email.to %></div>
                                                <div class="subject"><%= email.subject %></div>

                                                <%
                                                    const createdAt = new Date(email.createdAt);
                                                    const addLeadingZero = (number) => {
                                                    return number < 10 ? `0${number}` : number;
                                                    }
                                                    const formattedDate = `${createdAt.getDate()}-${addLeadingZero(createdAt.getMonth() + 1)}-${createdAt.getFullYear()}`;
                                                    const formattedTime = `${addLeadingZero(createdAt.getHours())}:${addLeadingZero(createdAt.getMinutes())}`;
                                                    const formattedDateTime = `${formattedDate} ${formattedTime}`;
                                                %>

                                                <div class="date" style="color: #FF9F00"><%= formattedDateTime %></div>
                                            </a>
                                        </div>
                                <% }); %>
                            <% } else { %>
                                <div>No emails found.</div>
                            <% } %>   
                        </div>                 
                        </div>

